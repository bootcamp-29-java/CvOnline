
<form>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="inputEmail4">First Name</label>
            <input type="email" class="form-control" id="inputEmail4" placeholder="First Name">
        </div>
        <div class="form-group col-md-6">
            <label for="inputPassword4">Last Name</label>
            <input type="password" class="form-control" id="inputPassword4" placeholder="Last Name">
        </div>
    </div>
    <div class="form-group">
        <label for="inputAddress">Email</label>
        <input type="text" class="form-control" id="inputAddress" placeholder="john@example.com">
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="inputEmail4">Birth Place</label>
            <input type="email" class="form-control" placeholder="Birth Place">
        </div>
        <div class="form-group col-md-6">
            <label for="inputPassword4">Birth Date</label>
            <input type="password" class="form-control" placeholder="Last Name">
        </div>
        <div class="form-group">
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1">
                <label class="form-check-label" for="inlineRadio1">Male</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option2">
                <label class="form-check-label" for="inlineRadio2">Female</label>
            </div>
        </div>



    </div>

    <div class="form-group">
        <label for="inputState">Nationality</label>
        <select id="inputState" class="form-control">
            <option selected>Choose...</option>
            <option>WNI</option>
            <option>WNA</option>
        </select>
    </div>



</form>
<button type="submit" class="btn btn-primary">Sign in</button>

